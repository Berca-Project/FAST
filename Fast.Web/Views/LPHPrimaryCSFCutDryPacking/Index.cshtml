													}
												}
												else
												{
													if (employee.EmployeeID == ViewBag.AccountSpvEmployeeID)
													{
														selected = "selected";
													}
												}
											}

											<option value="@(employee.EmployeeID)" @selected>[@(employee.EmployeeID)] @employee.FullName</option>
										}
									</select>
								</div>
							</div>
						</div>
					</div>
				</div>
