						//$(tr).find(".fa.fa-plus-circle").click();
						selected = null;
						break;
					case 2:
						info.shouldHide = true;
						const remarks = $("[name=WasteRemark]").val();
						if (remarks == "") info.shouldReturn = true;
						else {
							info.shouldHide = true;
