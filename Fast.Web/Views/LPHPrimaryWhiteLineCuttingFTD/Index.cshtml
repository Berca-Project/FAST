                    let opts = '<option value="Shiftly">Shiftly</option>';
                    for (let i = 0; i < productionreportData.length; i++) {
                        opts += `<option value="${productionreportData[i].BatchNo}">${productionreportData[i].BatchNo}</option>`;
                    }
                    opts += `<option value="${newData.BatchNo}">${newData.BatchNo}</option>`;
                    $(".WasteTypeByBatch").html(opts);
                    return newData;
                },
                onCancel() {
                    $("[name=BatchNo]").prop("disabled", false);
                },
                onEdit() {
                    $("[name=BatchNo]").prop("disabled", true);
                },
                onAdd(newData) {
                    //newData.Duration = countDuration(newData.Start, newData.Stop);
                    for (let i = 0; i < productionreportData.length; i++) {
                        if (productionreportData[i].BatchNo == newData.BatchNo) {
                            if (confirm("Batch sudah ada, replace data?")) { // Ngosongi input
                                productionreportData[i] = newData;
                                cancelEdit($("#get-pss-data"));
                                startupSetup('productionreport');
                            }
                            return null;
                        }
                    }
                    let opts = '<option value="Shiftly">Shiftly</option>';
                    for (let i = 0; i < productionreportData.length; i++) {
                        opts += `<option value="${productionreportData[i].BatchNo}">${productionreportData[i].BatchNo}</option>`;
                    }
                    opts += `<option value="${newData.BatchNo}">${newData.BatchNo}</option>`;
                    $(".WasteTypeByBatch").html(opts);
                    return newData;
                },
                onDelete(id) {
                    let opts = '<option value="Shiftly">Shiftly</option>';
                    for (let i = 0; i < productionreportData.length; i++) {
                        if (productionreportData[i].id != id)
                            opts += `<option value="${productionreportData[i].BatchNo}">${productionreportData[i].BatchNo}</option>`;
                    }
                    $(".WasteTypeByBatch").html(opts);
                },
                countSummary: function () {
                    let rowData = {
                        TotalWetTobCutting: 0,
                        TotalWetTobFTD: 0
                    };
                    for (var i = 0; i < productionreportData.length; i++) {
                        rowData.TotalWetTobCutting += parseFloat(productionreportData[i].TotalWetTobCutting);
                        rowData.TotalWetTobFTD += parseFloat(productionreportData[i].TotalWetTobFTD);
                    }
                    rowData.TotalWetTobCutting = rowData.TotalWetTobCutting.toFixed(2);
                    rowData.TotalWetTobFTD = rowData.TotalWetTobFTD.toFixed(2);
                    return rowData;
                },
            },
        }

