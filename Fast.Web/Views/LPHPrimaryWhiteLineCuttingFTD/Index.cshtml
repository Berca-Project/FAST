                            width: "400px",
                            type: 'error',
                            addclass: 'stack-bottomright',
                            hide: false,
                            buttons: {
                                sticker: false
                            }
                        });
                        setTimeout(function(){
            	            notice.remove();
            	        }, 3000);

                        notice.get().click(function () {
                            notice.remove();
                        });
                    }
                });
