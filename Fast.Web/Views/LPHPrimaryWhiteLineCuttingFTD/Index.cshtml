            $('#get-pss-data').click(function () {

                $('#loading_overlay').removeClass('d-none');
                $.ajax({
                    type: "POST",
                    url: "@Url.Action("getPSSData", "LPHPrimaryWhiteLineCuttingFTD")",
                    data: JSON.stringify({
                        BatchID: $('[name=BatchNo]').val()
                    }),
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (result) {
                        $('#loading_overlay').addClass('d-none');
                        if (result.Status == 'True') {
                            var model = JSON.parse(result.Data);
                            if (model.length > 0) {
                                var BatchID = $('[name=BatchNo]').val();
                                cancelEdit($("#get-pss-data"));
                                $('[name=BatchNo]').val(BatchID);
                                var WocIndex = 0;
                                for (var i = 0; i < model.length; i++) {

                                    if (model[i].EventType.trim() == 'Start_Batch Lamina Cutting') {
                                        var str = model[i].EventDateTimeLocal;
                                        const milliseconds = str.substring(str.lastIndexOf("(") + 1, str.lastIndexOf(")"));
                                        const time = moment(parseInt(milliseconds)).format("HH:mm");
                                        $('[name=StartCutting]').val(time);
                                    }
                                    if (model[i].EventType.trim() == 'End_Batch Lamina Cutting') {
                                        var str = model[i].EventDateTimeLocal;
                                        const milliseconds = str.substring(str.lastIndexOf("(") + 1, str.lastIndexOf(")"));
                                        const time = moment(parseInt(milliseconds)).format("HH:mm");
                                        $('[name=StopCutting]').val(time);
                                    }

                                    if (model[i].EventType.trim() == 'Start_Batch Lamina FTD U02') {
                                        var str = model[i].EventDateTimeLocal;
                                        const milliseconds = str.substring(str.lastIndexOf("(") + 1, str.lastIndexOf(")"));
                                        const time = moment(parseInt(milliseconds)).format("HH:mm");
                                        $('[name=StartFTD]').val(time);
                                    }
                                    if (model[i].EventType.trim() == 'End_Batch Lamina FTD U02') {
                                        var str = model[i].EventDateTimeLocal;
                                        const milliseconds = str.substring(str.lastIndexOf("(") + 1, str.lastIndexOf(")"));
                                        const time = moment(parseInt(milliseconds)).format("HH:mm");
                                        $('[name=StopFTD]').val(time);
                                    }
                                    switch (model[i].DataName) {
                                        case "Report.Cutter.BreakdownCount":
                                            $('[name=CutterStop]').val(model[i].Value);
                                            break;
                                        case "Report.Cutter.WOCReport":
                                            WocIndex++;
                                            if (WocIndex <= 3) {
                                                $('[name=WOCStart' + WocIndex + ']').val(model[i].Value);
                                            } else if (WocIndex <= 6) {
                                                $('[name=WOCMiddle' + (WocIndex-3) + ']').val(model[i].Value);
                                            } else if (WocIndex <= 9) {
                                                $('[name=WOCEnd' + (WocIndex-6) + ']').val(model[i].Value);
                                            }
                                            break;
                                        case "Blend":
                                            $('[name=Blend]').val(model[i].Value);
                                            break;
                                        case "Numeric Sap Batch ID":
                                            $('[name=SAPBatch]').val(model[i].Value);
                                            break;
                                        case "Report.WeighBelt400010.FlowAvg":
                                            $('[name=TobFlowAvgCutting]').val(toFloat(model[i].Value).toFixed(2));
                                            break;
                                        case "Report.WeighBelt400010.TotalWetTob":
                                            $('[name=TotalWetTobCutting]').val(model[i].Value);
                                            break;
                                        case "U02.FTDFlowSP.reAverageResPout":
                                            $('[name=TobFlowAvgFTD]').val(model[i].Value);
                                            break;
                                        case "U02.FTDwetQtyOnWeigherDs.reSumResPout":
                                            $('[name=TotalWetTobFTD]').val(model[i].Value);
                                            break;
                                        case "U02.FTDinletMoistDs.reAverageResPout":
                                            $('[name=InletOVAvg]').val(model[i].Value);
                                            break;
                                        case "U02.FTDoutletMoistDs.reAverageResPout":
                                            $('[name=ExitOVAvg]').val(model[i].Value);
                                            break;
                                        case "U02.FTDprocRetGasPressureDs.reAverageResPout":
                                            $('[name=PGPressure]').val(model[i].Value);
                                            break;
                                        case "U02.FTDprocGasTemperatureDs.reAverageResPout":
                                            $('[name=PGTempAvg]').val(model[i].Value);
                                            break;
                                        case "U02.FTDprocCombFurnaceTempDs.reAverageResPout":
                                            $('[name=BurnerTempAvg]').val(model[i].Value);
                                            break;
                                    };
                                }
                            } else {
                                var notice = new PNotify({
                                    title: "@UIResources.ErrorMessage",
                                    text: "Batch Tidak Ditemukan",
                                    width: "400px",
                                    type: 'error',
                                    addclass: 'stack-bottomright',
                                    hide: false,
                                    buttons: {
                                        sticker: false
                                    }
                                });

                                setTimeout(function(){
                                    notice.remove();
                                }, 3000);
                                notice.get().click(function () {
                                    notice.remove();
                                });
                            }
                        }
                        else {

                            var notice = new PNotify({
                                title: "@UIResources.ErrorMessage",
                                text: "Batch Tidak Ditemukan",
                                width: "400px",
                                type: 'error',
                                addclass: 'stack-bottomright',
                                hide: false,
                                buttons: {
                                    sticker: false
                                }
                            });

                            setTimeout(function(){
                	            notice.remove();
                	        }, 3000);
                            notice.get().click(function () {
                                notice.remove();
                            });
                        }
                    },
