                    for (var prop in data[i]) {
                        if (Object.prototype.hasOwnProperty.call(data[i], prop)) {
                            if (prop == "id") continue;
                            let newInsertData = {
                                HeaderName: tableId,
                                FieldName: prop,
                                Value: data[i][prop],
                                ValueType: tableColumnType[prop] == "number" ? "Numeric" : "NonNumeric",
