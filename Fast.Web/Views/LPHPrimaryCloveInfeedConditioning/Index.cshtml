                            if (wasteOpt[lph][indexCategory].child[i].kind == selected) {
                                $(tr).find("[name=Kind]").val(wasteOpt[lph][indexCategory].child[i].kind);
                                $(tr).find("[name=Type]").val(wasteOpt[lph][indexCategory].child[i].type);
                                $(tr).find("[name=Reused]").val(wasteOpt[lph][indexCategory].child[i].reused);
                                $(tr).find("[name=Weight]").val($('#WasteWeight').text());
                                break;
                            }
                        }
                        $(this).find(".next").show();
                        selected = null;
                        break;
                    case 3:
                        const remarks = $("[name=WasteRemark]").val();
                        if (remarks == "") info.shouldReturn = true;
                        else {
                            info.shouldHide = true;
                            $(tr).find("[name=Remarks]").val(remarks);
                            $(tr).find(".fa.fa-plus-circle").click();
                        }
                        break;

                }
            });
            $('#phase-waste-modal').on('hide', function (e) {
                selected = null;
            });
        });

	</script>
}



