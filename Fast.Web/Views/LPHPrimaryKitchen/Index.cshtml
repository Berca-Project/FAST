                                }
                            });

                            notice.get().click(function () {
                                notice.remove();
                            });
                        }
                        $('#loading_overlay').addClass('d-none');
                    },
                    complete: function (data) {
                        $('#loading_overlay').addClass('d-none');
                        setTimeout(function () {
                            window.location.href = "@(Url.Action("Index", "LPHPrimaryKitchen"))";
                        }, 1000);
                    }
                });
            }

