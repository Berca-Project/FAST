												$('[name=RefrationMin]').val(model[i].Value);
												break;
											case solCode + "REP_SolutionDensity":
												$('[name=SpecificActual]').val(model[i].Value);
												break;
											case "Specific Gravity at 25/25 °C - MAX":
											case solCode + "REC_SolutionDensityMax":
											case "REC.spesificGravityRe":
												$('[name=SpecificMax]').val(model[i].Value);
												break;
											case "Specific Gravity at 25/25 °C - MIN":
											case solCode + "REC_SolutionDensityMin":
											case "specificGravityRe":
												$('[name=SpecificMin]').val(model[i].Value);
												break;
											//case solCode + "REC_ProdCode_Step1":
											//    break;
											//case solCode + "REC_Oper_Step2":
											//    break;
										};
                                    }
                                }
                                if (@ViewBag.isWest == 0) {
                                    for (var ir = 1; ir <= 25; ir++) {
                                        //console.log("Data ke " + ir);
                                        const resultAddingMaterial = model.find(({ DataName }) => DataName === "Steps[" + ir + "].IngredientCodeSt");
                                        //console.log("Data ke " + ir + " : " + resultAddingMaterial);
                                        if (resultAddingMaterial != null)
                                            if (resultAddingMaterial.Value != 0) {
                                                //console.log("Value Data ke " + ir + " : " + resultAddingMaterial.Value);
                                                const resultActualQty = model.find(({ DataName }) => DataName === "Steps[" + ir + "].ActualQty");
                                                const resultQtyRecipe = model.find(({ DataName }) => DataName === "Steps[" + ir + "].QtyRe");
                                                const resultSatuan = model.find(({ DataName }) => DataName === "Steps[" + ir + "].UnitSt");
                                                //console.log("DataName " + ir + " : " + "Steps[" + ir + "].UnitSt :: " + resultSatuan.Value);
                                                const resultType = model.find(({ DataName }) => DataName === "Steps[" + ir + "].OpsCodeIt");
                                                var addType = resultType.Value == 10 ? 'Auto' : 'Manual';
                                                ingredientData.push({
                                                    ActualQty: resultActualQty ? resultActualQty.Value : '',
                                                    AddingMaterial: resultAddingMaterial ? resultAddingMaterial.Value : '',
                                                    AddingType: addType,
                                                    QtyRecipe: resultQtyRecipe ? resultQtyRecipe.Value : '',
                                                    Satuan: resultSatuan ? resultSatuan.Value : '',
                                                    CekActual: false,
                                                    StatusAdding: false,
                                                    BatchMaterial: '',
                                                    id: uuidv4(),
                                                });
                                            }
                                    }
                                } else {
                                    for (var ir = 1; ir <= 20; ir++) {
                                        const resultAddingMaterial = model.find(({ DataName }) => DataName === solCode + "REC_ProdCode_Step" + ir);
                                        if (resultAddingMaterial.Value != 0) {
                                            const resultDataType = model.find(({ DataName }) => DataName === solCode + "REC_Oper_Step" + ir);
                                            const resultQtyRecipe = model.find(({ DataName }) => DataName === solCode + "REC_Param_Step" + ir);
                                            const resultSatuan = model.find(({ DataName }) => DataName === solCode + "REC_Unit_Step" + ir);
                                            const resultActualQty = model.find(({ DataName }) => DataName === solCode + "REP_RealParam_Step" + ir);
                                            var addType = '';
                                            switch (toInt(resultDataType.Value)) {
                                                case 1:
                                                    addType = 'Start Process';
                                                    break;
                                                case 2:
                                                    addType = 'End Process';
                                                    break;
                                                case 3:
                                                    addType = 'Start Mixer';
                                                    break;
                                                case 4:
                                                    addType = 'Stop Mixer';
                                                    break;
                                                case 5:
                                                    addType = 'Mixer Active for a Period';
                                                    break;
                                                case 6:
                                                    addType = 'Start Warming';
                                                    break;
                                                case 7:
                                                    addType = 'Stop Warming';
                                                    break;
                                                case 8:
                                                    addType = 'Check Solution';
                                                    break;
                                                case 9:
                                                    addType = 'Manual';
                                                    break;
                                                case 10:
                                                    addType = 'Auto';
                                                    break;
                                                case 11:
                                                    addType = 'Prepare Powder';
                                                    break;
                                                case 12:
                                                    addType = 'Automatic Liquid Adding for Powder';
                                                    break;
                                                case 13:
                                                    addType = 'Washing Powder Line';
                                                    break;
                                                case 14:
                                                    addType = 'Warming Active for a Period';
                                                    break;
                                                case 15:
                                                    addType = 'Standby';
                                                    break;
                                            }
                                            ingredientData.push({
                                                ActualQty: resultActualQty.Value,
                                                AddingMaterial: resultAddingMaterial.Value,
                                                AddingType: addType,
                                                QtyRecipe: resultQtyRecipe.Value,
                                                Satuan: resultSatuan.Value,
                                                CekActual: false,
                                                StatusAdding: false,
                                                BatchMaterial: '',
                                                id: uuidv4(),
                                            });
                                        }
                                    }
                                }
                                startupSetup('ingredient');
                                $('[name=SolutionDetail]').trigger("change");
                            } else {
                                var notice = new PNotify({
                                    title: "@UIResources.ErrorMessage",
                                    text: "Batch Tidak Ditemukan",
                                    width: "400px",
                                    type: 'error',
                                    addclass: 'stack-bottomright',
                                    hide: false,
                                    buttons: {
                                        sticker: false
                                    }
                                });

                                setTimeout(function(){
                                    notice.remove();
                                }, 3000);
                                notice.get().click(function () {
                                    notice.remove();
                                });
                            }
                        }
                        else {

                            var notice = new PNotify({
                                title: "@UIResources.ErrorMessage",
                                text: "Batch Tidak Ditemukan",
                                width: "400px",
                                type: 'error',
                                addclass: 'stack-bottomright',
                                hide: false,
                                buttons: {
                                    sticker: false
                                }
                            });

                            setTimeout(function(){
                	            notice.remove();
                	        }, 3000);
                            notice.get().click(function () {
                                notice.remove();
                            });
                        }
                    },
                    error: function (result) {

                        var notice = new PNotify({
                            title: "@UIResources.ErrorMessage",
                            text: "Batch Tidak Ditemukan",
                            width: "400px",
                            type: 'error',
                            addclass: 'stack-bottomright',
                            hide: false,
                            buttons: {
                                sticker: false
                            }
                        });
                        setTimeout(function(){
            	            notice.remove();
            	        }, 3000);

                        notice.get().click(function () {
                            notice.remove();
                        });
                    }
                });
                //for (var prop in pssData) {
                //    if (Object.prototype.hasOwnProperty.call(pssData, prop)) {
                //        setValue($("[name=" + prop + "]"), pssData[prop]);
