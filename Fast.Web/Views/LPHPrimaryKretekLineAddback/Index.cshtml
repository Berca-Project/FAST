            dsl: {
                data: dslData,
                //onValidate: notEmptyValidation,
            },
            smallLamina: {
                data: smallLaminaData,
                //onValidate: notEmptyValidation,
            },
            csf: {
                data: csfData,
                //onValidate: notEmptyValidation,
            },
            clove: {
                data: cloveData,
                //onValidate: notEmptyValidation,
            },
            flavourApplication: {
                data: flavourApplicationData,
                //onValidate: notEmptyValidation,
            },
            addback: {
                data: addbackData,
                mode: TableMode.SPREAD,
                onSave(newData) {
                    $("[name=BatchNo]").prop("disabled", false);
                    newData.Duration = countDuration(newData.Start, newData.Stop);
                    return newData;
                },
                onCancel() {
                    $("[name=BatchNo]").prop("disabled", false);
                },
                onEdit() {
                    $("[name=BatchNo]").prop("disabled", true);
                },
                onAdd(newData) {
                    newData.Duration = countDuration(newData.Start, newData.Stop);
                    for (let i = 0; i < addbackData.length; i++) {
                        if (addbackData[i].BatchNo == newData.BatchNo) {
                            if (confirm("Batch sudah ada, replace data?")) {
                                addbackData[i] = newData;
                                cancelEdit($("#get-pss-data"));
                                startupSetup("addback");
                            }
                            return null;
                        }
                    }
                    return newData;
                },
                onCollectDataSingle(data) {
                    switch (data.FieldName) {
                        case "oos":
                        case "rtc":
                        case "cres":
                        case "rs":
                        case "dsl":
                        case "csf":
                        case "clove":
                        case "smallLamina":
                        case "flavourApplication":
                            data.ValueType = "Json";
                            break;
                    }
                    return data;
                },
                onPlaceData(data) {
                    data.oos = JSON.parse(data.oos || "{}");
                    data.rtc = JSON.parse(data.rtc || "{}");
                    data.cres = JSON.parse(data.cres || "{}");
                    data.rs = JSON.parse(data.rs || "{}");
                    data.dsl = JSON.parse(data.dsl || "{}");
                    data.csf = JSON.parse(data.csf || "{}");
                    data.clove = JSON.parse(data.clove || "{}");
                    data.smallLamina = JSON.parse(data.smallLamina || "{}");
                    data.flavourApplication = JSON.parse(data.flavourApplication || "{}");
                    return data;
                },
                countSummary: function () {
                    let rowData = {};
                    let Duration = 0;
                    for (var i = 0; i < addbackData.length; i++) {
                        Duration += parseInt(addbackData[i].Duration);
                    }
                    rowData.Duration = Duration;
                    return rowData;
                },
            },
        }
    </script>

