@section Scripts{
	<!-- Specific Page Vendor -->
	<script src="~/Content/dashforge/lib/prismjs/prism.js"></script>
	<script src="~/Content/dashforge/lib/moment/min/moment-with-locales.min.js"></script>
	<script src="~/Content/dashforge/lib/datatables.net/js/jquery.dataTables.min.js"></script>
	<script src="~/Content/dashforge/lib/datatables.net-dt/js/dataTables.dataTables.min.js"></script>
	<script src="~/Content/dashforge/lib/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
	<script src="~/Content/dashforge/lib/datatables.net-responsive-dt/js/responsive.dataTables.min.js"></script>

	<script type="text/javascript">

		var pcID = 0;
		var locationId = 0;
		var locationType = 'country';

		$('#btnUpload').on('click', function () {
			if (@Model.SubDepID == @Model.DepID) {
				$('#labelDepartment').hide();
				$('#inputDepartment').hide();
				$.ajax({
					type: "post",
					url: fastApp.BaseUrl + "/MPP/GetSubDepartmentByDepartmentID",
					data: { id: @Model.SubDepID },
					datatype: "json",
					traditional: true,
					success: function (data) {
						var ddlTemp = "<select id='ddlSubDepartment'>";
						for (var i = 0; i < data.length; i++) {
							ddlTemp = ddlTemp + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
						}
						ddlTemp = ddlTemp + '</select>';
						$('#ddlSubDepartment').html(ddlTemp);
					}
				});
			} else if (@Model.SubDepID != @Model.PcID && @Model.SubDepID != @Model.DepID) {
				$('#labelDepartment').hide();
				$('#inputDepartment').hide();
				$('#labelSubDepartment').hide();
				$('#inputSubDepartment').hide();
			} else {
				$.ajax({
					type: "post",
					url: fastApp.BaseUrl + "/MPP/GetDepartmentByProdCenterID",
					data: { id: @Model.SubDepID },
					datatype: "json",
					traditional: true,
					success: function (data) {
						var ddlTemp = "<select id='ddlDepartment'>";
						for (var i = 0; i < data.length; i++) {
							ddlTemp = ddlTemp + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
						}
						ddlTemp = ddlTemp + '</select>';
						$('#ddlDepartment').html(ddlTemp);
						$('#ddlSubDepartment').html("<select id='ddlSubDepartment'><option value='0'>- Please Select -</option></select>");
					}
				});
			}
		});

		$('#btnExport').on('click', function () {
			if ($('#flDate').val() == '') {
				alert('Please select the date first');
			} else if ($('#ddlProdCenter1').val() == '0') {
				alert('Please select the production center first');
			}
		});

		function GenerateExcel() {
			if ($('#flDate').val() == '') {
				alert('Please select the date first');
			} else if ($('#ddlProdCenter1').val() == '0') {
				alert('Please select the production center first');
			} else {
				var notice = new PNotify({
					title: "Generating Excel file",
					text: "Please wait a few seconds....",
					addclass: 'stack-bottomright',
					width: "400px",
					type: 'success',
					stack: stack_bottomright,
				});

				notice.get().click(function () {
					notice.remove();
				});

				var dateFilter = $('#flDate').val();
				var pcID = $('#ddlProdCenter1').val();

				if ($('#ddlProdCenter1').val() == '0') {
					locationId = $('#ddlCountry1').val();
					locationType = 'country';
				} else if ($('#ddlDepartment1').val() == '0') {
					locationId = $('#ddlProdCenter1').val();
					locationType = 'productioncenter';
				} else if ($('#ddlSubDepartment1').val() == '0') {
					locationId = $('#ddlDepartment1').val();
					locationType = 'department';
				} else {
					locationId = $('#ddlSubDepartment1').val();
					locationType = 'subdepartment';
				}

				location.href = '@Url.Action("GenerateExcel", "MPP")?dateFilter=' + dateFilter + '&locID=' + locationId + '&locType=' + locationType;
			}
        };

		$('#btnSearch').on('click', function () {
			if ($('#flDate').val() == '') {
				alert('Please select the date first');
			} else if ($('#ddlProdCenter1').val() == '0') {
				alert('Please select the production center first');
			} else {
				var dateFilter = $('#flDate').val();
				pcID = $('#ddlProdCenter1').val();

				if ($('#ddlDepartment1').val() == '0') {
					locationId = $('#ddlProdCenter1').val();
					locationType = 'productioncenter';
				} else {
					locationId = $('#ddlDepartment1').val();
					locationType = 'department';
				}

				$('#loading_overlay').removeClass('d-none');
				$.ajax({
					type: "post",
					url: fastApp.BaseUrl + "/MPP/GetAllMachineList",
					data: { dateFilter, locId: locationId, locType: locationType, pcID },
					datatype: "json",
					traditional: true,
					success: function (data) {
						$('#loading_overlay').addClass('d-none');

						$('#dateSP1').text(data.CurrentDate);
						$('#dateSP2').text(data.NextDate);
						$('#overallSP1').text(data.SelectedDateOverallPercentage);
						$('#datePP1').text(data.CurrentDate);
						$('#datePP2').text(data.NextDate);
						$('#overallSP2').text(data.NextDateOverallPercentage);

                        //shift 1
						var blokSP = "<h6>" + data.SelectedDateShift1Percentage + "</h6><div class='row'>";
						var blokPP = "<h6>SHIFT 1</h6><div class='row'>";
						var blokSP2 = "<h6>" + data.NextDateShift1Percentage + "</h6><div class='row'>";
						var blokPP2 = "<h6>SHIFT 1</h6><div class='row'>";

						for (var i = 0; i < data.MachineList.length; i++) {
							if (data.MachineList[i].IsSelectedDate) {
								if (data.MachineList[i].IsSP) {
									if (!data.MachineList[i].IsExistInWpp) {
										blokSP = blokSP + '<button type="button" class="btn btn-secondary marginBox" data-toggle="modal" data-target="#modalDetailMpp" onClick="Detail(\'' + data.MachineList[i].Code + '\',1)">' + data.MachineList[i].Code + '</button>';
									} else if (data.MachineList[i].IsShift1Zero) {
										blokSP = blokSP + '<button type="button" class="btn btn-dark marginBox" data-toggle="modal" data-target="#modalDetailMpp" onClick="Detail(\'' + data.MachineList[i].Code + '\',1)">' + data.MachineList[i].Code + '</button>';
									} else if (data.MachineList[i].IsShift1FullyAssigned) {
										blokSP = blokSP + '<button type="button" class="btn btn-success marginBox" data-toggle="modal" data-target="#modalDetailMpp" onClick="Detail(\'' + data.MachineList[i].Code + '\',1)">' + data.MachineList[i].Code + '</button>';
									} else {
										blokSP = blokSP + '<button type="button" class="btn btn-danger marginBox" data-toggle="modal" data-target="#modalDetailMpp" onClick="Detail(\'' + data.MachineList[i].Code + '\',1)">' + data.MachineList[i].Code + '</button>';
									}
								} else {
									if (!data.MachineList[i].IsExistInWpp) {
										blokPP = blokPP + '<button type="button" class="btn btn-secondary marginBox" data-toggle="modal" data-target="#modalDetailMpp" onClick="Detail(\'' + data.MachineList[i].Code + '\',1)">' + data.MachineList[i].Code + '</button>';
									} else if (data.MachineList[i].IsShift1FullyAssigned) {
										blokPP = blokPP + '<button type="button" class="btn btn-success marginBox" data-toggle="modal" data-target="#modalDetailMpp" onClick="Detail(\'' + data.MachineList[i].Code + '\',1)">' + data.MachineList[i].Code + '</button>';
									} else {
										blokPP = blokPP + '<button type="button" class="btn btn-danger marginBox" data-toggle="modal" data-target="#modalDetailMpp" onClick="Detail(\'' + data.MachineList[i].Code + '\',1)">' + data.MachineList[i].Code + '</button>';
									}
								}
							} else {
								if (data.MachineList[i].IsSP) {
									if (!data.MachineList[i].IsExistInWpp) {
										blokSP2 = blokSP2 + '<button type="button" class="btn btn-secondary marginBox" data-toggle="modal" data-target="#modalDetailMpp" onClick="Detail2(\'' + data.MachineList[i].Code + '\',1)">' + data.MachineList[i].Code + '</button>';
									} else if (data.MachineList[i].IsShift1Zero) {
										blokSP2 = blokSP2 + '<button type="button" class="btn btn-dark marginBox" data-toggle="modal" data-target="#modalDetailMpp" onClick="Detail2(\'' + data.MachineList[i].Code + '\',1)">' + data.MachineList[i].Code + '</button>';
									} else if (data.MachineList[i].IsShift1FullyAssigned) {
										blokSP2 = blokSP2 + '<button type="button" class="btn btn-success marginBox" data-toggle="modal" data-target="#modalDetailMpp" onClick="Detail2(\'' + data.MachineList[i].Code + '\',1)">' + data.MachineList[i].Code + '</button>';
									} else {
										blokSP2 = blokSP2 + '<button type="button" class="btn btn-danger marginBox" data-toggle="modal" data-target="#modalDetailMpp" onClick="Detail2(\'' + data.MachineList[i].Code + '\',1)">' + data.MachineList[i].Code + '</button>';
									}
								} else {
									if (!data.MachineList[i].IsExistInWpp) {
