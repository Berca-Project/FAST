@model Fast.Web.Models.MppModel
@using Fast.Web.Resources;

@section CSS_lib{
	<!-- Specific Page Vendor CSS -->
	<link href="~/Content/dashforge/lib/typicons.font/typicons.css" rel="stylesheet">
	<link href="~/Content/dashforge/lib/prismjs/themes/prism-vs.css" rel="stylesheet">
	<link href="~/Content/dashforge/lib/datatables.net-dt/css/jquery.dataTables.min.css" rel="stylesheet">
	<link href="~/Content/dashforge/lib/datatables.net-responsive-dt/css/responsive.dataTables.min.css" rel="stylesheet">
	<link href="~/Content/dashforge/assets/css/dashforge.demo.css" rel="stylesheet" />
	<link href="~/Content/dashforge/assets/css/dashforge.dashboard.css" rel="stylesheet" />
}

@section Styles{
	<style>
		.marginBox {
			margin: 5px 5px 5px 5px;
		}
	</style>
}

<div class="modal fade" id="modalUploadMpp" tabindex="-1" role="dialog" aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-content">
			@using (Html.BeginForm("Upload", "Mpp", FormMethod.Post, new { encType = "multipart/form-data", @id = "UploadForm" }))
			{
				@Html.AntiForgeryToken()
				<div class="modal-header pd-y-20 pd-x-20 pd-sm-x-50">
					<div class="media align-items-center">
						<span class="tx-color-03 d-none d-sm-block"><i data-feather="share" class="wd-60 ht-60"></i></span>
						<div class="media-body mg-sm-l-20">
							<h4 class="tx-18 tx-sm-20 mg-b-2">@UIResources.Upload</h4>
							<p class="tx-13 tx-color-03 mg-b-0">@UIResources.MppDataManagement</p>
						</div>
					</div><!-- media -->
				</div><!-- modal-header -->
				<div class="modal-body pd-sm-t-30 pd-sm-b-40 pd-sm-x-30">
					<div class="form-group ">
						<div class="form-control-static">
							<span class="btn btn-success fileinput-button btn-block">
								@Html.TextBoxFor(x => x.PostedFilename, null, new { @type = "file", @accept = ".xls, .xlsx", @required = "required", @id = "customFile" })
							</span>
						</div>
						<br />
						<div class="progress" id="loadingbar" style="height: 30px;">
							<div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">Please wait a few seconds...</div>
						</div>
					</div>
				</div>
				<div class="modal-footer pd-x-20 pd-y-15">
					<button type="button" class="btn btn-white" data-dismiss="modal" id="btnCancel">@UIResources.Cancel</button>
					<button type="button" class="btn btn-primary" id="btnSubmit" onclick="OnSubmit()">@UIResources.Upload</button>
				</div>
			}
		</div><!-- modal-content -->
	</div><!-- modal-dialog -->
</div><!-- modal -->

<div class="modal fade" id="modalDownloadMpp" tabindex="-1" role="dialog" aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered" role="document">
		<div class="modal-content">
			@using (Html.BeginForm("GenerateExcel", "Mpp", FormMethod.Post, new { @class = "form-horizontal", @id = "idmodalDownloadMpp" }))
			{
				@Html.AntiForgeryToken()
				<div class="modal-header pd-y-20 pd-x-20 pd-sm-x-50">
					<a href="" role="button" class="close pos-absolute t-15 r-15" data-dismiss="modal" aria-label="Close">
						<span aria-hidden="true">&times;</span>
					</a>
					<div class="media align-items-center">
						<span class="tx-color-03 d-none d-sm-block"><i data-feather="download" class="wd-60 ht-60"></i></span>
						<div class="media-body mg-sm-l-20">
							<h4 class="tx-18 tx-sm-20 mg-b-2">@UIResources.DownloadTemplate</h4>
							<p class="tx-13 tx-color-03 mg-b-0">@UIResources.MppDataManagement</p>
						</div>
					</div><!-- media -->
				</div><!-- modal-header -->
				<div class="modal-body pd-sm-t-30 pd-sm-b-40 pd-sm-x-30">
					<div class="row row-sm mg-b-10">
						<div class="col-4">
							<label class="tx-10 tx-uppercase tx-medium tx-spacing-1 mg-b-5 tx-color-03">@UIResources.StartDate</label>
						</div>
						<div class="col-8">
							@Html.TextBoxFor(x => x.StartDateCalendar, null, new { @class = "form-control", @id = "startDate", @required = "required", @autocomplete = "off" })
						</div>
					</div>
					<div class="row row-sm mg-b-10">
						<div class="col-4">
							<label class="tx-10 tx-uppercase tx-medium tx-spacing-1 mg-b-5 tx-color-03">@UIResources.EndDate</label>
						</div>
						<div class="col-8">
							@Html.TextBoxFor(x => x.EndDateCalendar, null, new { @class = "form-control", @id = "endDate", @required = "required", @autocomplete = "off" })
						</div>
					</div>
					<div class="row row-sm mg-b-10">
						<div class="col-4">
							<label class="tx-10 tx-uppercase tx-medium tx-spacing-1 mg-b-5 tx-color-03">@UIResources.GroupType</label>
						</div>
						<div class="col-8">
							@Html.DropDownListFor(x => x.GroupTypeID, (IEnumerable<SelectListItem>)ViewBag.GroupTypeList, new { @class = "form-control", @id = "ddlGroupType" })
						</div>
					</div>
				</div>
				<div class="modal-footer pd-x-20 pd-y-15">
					<button type="button" class="btn btn-white" data-dismiss="modal">@UIResources.Cancel</button>
					<button type="submit" class="btn btn-primary">@UIResources.Download</button>
				</div>
			}
		</div><!-- modal-content -->
	</div><!-- modal-dialog -->
</div><!-- modal -->

