			$.ajax({
				type: "post",
				url: fastApp.BaseUrl + "/WppPrimary/GetWeekByYear",
				data: { year: $('#ddlStartYearFilter').val() },
				datatype: "json",
				traditional: true,
				success: function (data) {
					var ddlTemp = "<select id='ddlWeek'>";
					for (var i = 0; i < data.length; i++) {
						ddlTemp = ddlTemp + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
					}
					ddlTemp = ddlTemp + '</select>';
					$('#ddlStartWeekFilter').html(ddlTemp);
				}
			});
