@model Fast.Web.Models.Report.InputDailyModel
@using Fast.Web.Resources;
@{
    ViewBag.Title = "Index";
}

@section Styles{
    <style>
        table.dataTable thead .sorting_asc, table.dataTable thead .sorting_desc {
            background-color: transparent
        }
    </style>
}
<div class="content content-fixed lph-form">
    <div class="container-fluid pd-x-0 pd-lg-x-10 pd-xl-x-0">
        <div class="d-sm-flex align-items-center justify-content-between mg-b-20 mg-lg-b-25 mg-xl-b-30">
            <div>
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb breadcrumb-style1 mg-b-10">
                        @*<li class="breadcrumb-item"><a href="#">Report</a></li>*@
                        <li class="breadcrumb-item active" aria-current="page">Input Shift Daily</li>
                    </ol>
                </nav>
                <h4 class="mg-b-0 tx-spacing--1">Input Shift Daily</h4>
            </div>
            <div class="d-none d-md-block">
                <button class="btn btn-primary btn-sm pd-x-15 btn-uppercase dtAddButton" onclick="location.href='@Url.Action("Index", "ShiftDaily")'">
                    <i data-feather="arrow-left-circle" class="mg-r-5"></i> @UIResources.Back
                </button>

                <div data-toggle="modal" data-target="#modalGenerate" class="btn btn-sm pd-x-15 btn-outline-success btn-uppercase mg-l-5" id="btnWorkingTime"><i class="fas fa-file-csv"></i> Generate</div>
                <div data-toggle="modal" data-target="#modalUpload" class="btn btn-sm pd-x-15 btn-success btn-uppercase mg-l-5" id="btnWorkingTime"><i data-feather="upload" class="wd-10 mg-r-5"></i> Upload</div>
                @*<a href="@(Url.Action("Create","InputDaily"))" class="btn btn-sm pd-x-15 btn-primary btn-uppercase mg-l-5" id="btnWorkingTime"><i data-feather="plus-circle" class="wd-10 mg-r-5"></i> New</a>*@
            </div>
        </div>
        <div class="col-12 mg-b-20">
            <div class="card collect-component" id="Information">
                <div class="card-header bg-primary tx-white tx-center font-weight-bold">Information</div>
                <div class="card-body d-flex">
                    <div style="flex:2">
                        <div class="form-group d-flex">
                            <label class="col-2" style="flex:1">Date</label>
                            <div style="flex:1">
                                <div class="input-group col-4">

                                    @Html.TextBoxFor(x => x.Date, "{0:yyyy-mm-dd}", new { @class = "form-control", @id = "tbDate", autocomplete = "off" })
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="divider-text divider-vertical"></div>
                    <div class="flex-1">
                        <div class="form-group row">
                            <label class="col-5 col-form-label">Prod. Center</label>
                            <div class="col-7">
                                @Html.DropDownListFor(x => x.ProdCenterID, (IEnumerable<SelectListItem>)ViewBag.ProductionCenterList, new { @class = "form-control", @id = "ddlProdCenter" })
                            </div>
                        </div>
                    </div>
                    <div class="divider-text divider-vertical"></div>
                    <div class="flex-1">
                        <div class="form-group row">
                            <label class="col-4 col-form-label">Machine</label>
                            <div class="col-6">
                                <select class="form-control select2" name="machine">
                                    <option label="Select Machine"></option>
                                </select>
                            </div>
                        </div>
                    </div>

                    <div class="flex-1" style="max-width:50px;">
                        <div class="form-group row">
                            <button id="btnSearch" class="btn btn-sm  btn-primary btn-uppercase pd-y-3"><i data-feather="search"></i></button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-12 mg-b-20">
            <div class="card">
                <div class="card-header bg-primary tx-white tx-center font-weight-bold">
                    Input Shift Daily
                </div>
                <div class="card-body">
                    <div class="tableWrapper" style="max-height: 350px">
                        <table class="table table-bordered table-hover table-striped" id="inputList" style="display:block;overflow-x:auto; white-space:nowrap;">
                            <thead class="thead-info">
                                <tr class="tx-center">
                                    <th style="min-width:95px;">ID</th>
                                    <th style="min-width:95px;">Production Centre</th>
                                    <th style="min-width:95px;">Tgl</th>
                                    <th style="min-width:95px;">Shift</th>
                                    <th style="min-width:95px;">LU</th>
                                    <th style="min-width:95px;">MTBF - Value</th>
                                    <th style="min-width:95px;">MTBF - Focus</th>
                                    <th style="min-width:95px;">MTBF - Action Plan</th>
                                    <th style="min-width:95px;">CPQI - Value</th>
                                    <th style="min-width:95px;">CPQI - Focus</th>
                                    <th style="min-width:95px;">CPQI - Action Plan</th>
                                    <th style="min-width:95px;">VQI - Value</th>
                                    <th style="min-width:95px;">VQI - Focus</th>
                                    <th style="min-width:95px;">VQI - Action Plan</th>
                                    <th style="min-width:95px;">Working Time - Value</th>
                                    <th style="min-width:95px;">Working Time - Focus</th>
                                    <th style="min-width:95px;">Working Time - Action Plan</th>
                                    <th style="min-width:95px;">Uptime - Value</th>
                                    <th style="min-width:95px;">Uptime - Focus</th>
                                    <th style="min-width:95px;">Uptime - Action Plan</th>
                                    <th style="min-width:95px;">STRS - Value</th>
                                    <th style="min-width:95px;">STRS - Focus</th>
                                    <th style="min-width:95px;">STRS - Action Plan</th>
                                    <th style="min-width:95px;">Production Volume - Value</th>
                                    <th style="min-width:95px;">Production Volume - Focus</th>
                                    <th style="min-width:95px;">Production Volume - Action Plan</th>
                                    <th style="min-width:95px;">CRR - Value</th>
                                    <th style="min-width:95px;">CRR - Focus</th>
                                    <th style="min-width:95px;">CRR - Action Plan</th>
                                    <th style="min-width:40px;">Action</th>
                                </tr>
                            </thead>
                            <tbody id="inputdaily">
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

    </div><!-- row -->
</div><!-- container -->



