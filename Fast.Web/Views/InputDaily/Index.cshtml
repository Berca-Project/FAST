        </div>
                <div class="modal-footer pd-x-20 pd-y-15">
                    <button type="button" class="btn btn-white" data-dismiss="modal">Cancel</button>
                    <button type="submit" class="btn btn-success">@UIResources.DownloadTemplate</button>
                </div>
            }
        </div><!-- modal-content -->
    </div><!-- modal-dialog -->
</div><!-- modal Generate-->

<div class="modal fade" id="modalUpload" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            @using (Html.BeginForm("Upload", "InputDaily", FormMethod.Post, new { encType = "multipart/form-data", @id = "UploadForm" }))
            {
                @Html.AntiForgeryToken()
                <div class="modal-header pd-y-20 pd-x-20 pd-sm-x-50">
                    <a href="" role="button" class="close pos-absolute t-15 r-15" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </a>
                    <div class="media align-items-center">
                        <span class="tx-color-03 d-none d-sm-block"><i data-feather="share" class="wd-60 ht-60"></i></span>
                        <div class="media-body mg-sm-l-20">
                            <h4 class="tx-18 tx-sm-20 mg-b-2">Upload</h4>
                            <p class="tx-13 tx-color-03 mg-b-0">Upload for Input Daily</p>
                        </div>
                    </div>
                </div>
                <div class="modal-body pd-sm-t-30 pd-sm-b-40 pd-sm-x-30">

                    @*<div class="form-group row">
                        <div class="col-4">
                            <label class="tx-10 tx-uppercase tx-medium tx-spacing-1 mg-t-10 tx-color-03">@UIResources.ProductionCenter</label>
                        </div>
                        <div class="col-8">
                            @Html.DropDownListFor(x => x.ProdCenterID, (IEnumerable<SelectListItem>)ViewBag.ProductionCenterList, new { @class = "form-control", @id = "ddlProdCenter2" })
                        </div>
                    </div>*@

                    <div class="form-group row">
                        <div class="col-4">
                            <label class="tx-10 tx-uppercase tx-medium tx-spacing-1 mg-t-10 tx-color-03">Date</label>
                        </div>
                        <div class="col-8">
                            @Html.TextBoxFor(x => x.Date, "{0:yyyy-mm-dd}", new { @class = "form-control", @id = "tbDateStart", @required = "required", autocomplete = "off" })
                        </div>
                    </div>
                    <div class="form-control-static">
                        <span class="btn btn-success fileinput-button btn-block">
                            @Html.TextBoxFor(x => x.PostedFilename, null, new { @type = "file", @accept = ".xls, .xlsx", @required = "required", @id = "customFile" })
                        </span>
                    </div>
                    <br />
                    <div class="progress" id="loadingbar" style="height: 30px;">
                        <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">Please wait a few seconds...</div>
                    </div>
                </div>
                <div class="modal-footer pd-x-20 pd-y-15">
                    <button type="button" class="btn btn-white" data-dismiss="modal" id="btnCancel">@UIResources.Cancel</button>
                    <button type="button" class="btn btn-primary" id="btnSubmit" onclick="OnSubmit()">@UIResources.Upload</button>
                </div>
            }
        </div><!-- modal-content -->
    </div><!-- modal-dialog -->
</div><!-- modal Upload-->

<div class="modal fade" id="deleteModal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered wd-sm-650" role="document">
        <div class="modal-content">
            <div class="modal-header pd-y-20 pd-x-20 pd-sm-x-30">
                <a href="" role="button" class="close pos-absolute t-15 r-15" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </a>
                <div class="media align-items-center">
                    <span class="tx-color-03 d-none d-sm-block"><i data-feather="x-circle" class="wd-60 ht-60"></i></span>
                    <div class="media-body mg-sm-l-20">
                        <h4 class="tx-18 tx-sm-20 mg-b-2">@UIResources.Confirmation</h4>
                    </div>
                </div><!-- media -->
            </div><!-- modal-header -->
            <div class="modal-body">
                <div class="form-group">
                    <label class="tx-10 tx-uppercase tx-medium tx-spacing-1 mg-b-5 tx-color-03" id="deleteconfirm"></label>
                </div>
            </div><!-- modal-body -->
            <div class="modal-footer pd-x-20 pd-y-15">
                <button type="button" class="btn btn-white" data-dismiss="modal" id="modal-btn-cancel">@UIResources.NO</button>
                <button type="submit" class="btn btn-primary" id="modal-btn-ok">@UIResources.YES</button>
            </div>
        </div><!-- modal-content -->
    </div><!-- modal-dialog -->
</div><!-- modal -->
@section Scripts{
    <script src="~/Content/dashforge/lib/feather-icons/feather.min.js"></script>
    <script src="~/Content/dashforge/lib/perfect-scrollbar/perfect-scrollbar.min.js"></script>
    <script src="~/Content/dashforge/lib/jquery.flot/jquery.flot.js"></script>
    <script src="~/Content/dashforge/lib/jquery.flot/jquery.flot.stack.js"></script>
    <script src="~/Content/dashforge/lib/jquery.flot/jquery.flot.resize.js"></script>
    <script src="~/Content/dashforge/lib/chart.js/Chart.bundle.min.js"></script>

    <script src="~/Content/dashforge/lib/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Content/dashforge/lib/datatables.net-dt/js/dataTables.dataTables.min.js"></script>
    <script src="~/Content/dashforge/lib/datatables.net-responsive/js/dataTables.responsive.min.js"></script>
    <script src="~/Content/dashforge/lib/datatables.net-responsive-dt/js/responsive.dataTables.min.js"></script>
    <script src="~/Content/dashforge/lib/cleave.js/cleave.min.js"></script>
    <script src="~/Content/dashforge/lib/select2/js/select2.min.js"></script>
    <script src="~/Content/dashforge/lib/moment/moment.js"></script>

    <script>
        var result = "@ViewBag.Result";
		    if (result != "") {
			    if (result == "True") {
				    var message = "@ViewBag.Message";

				    var notice = new PNotify({
					    title: "@UIResources.Confirmation",
					    text: message,
					    width: "400px",
					    type: 'success',
					    addclass: 'stack-bottomright',
					    delay: 2000
				    });

				    notice.get().click(function () {
					    notice.remove();
				    });
			    }
			    else {
				    var error = "@ViewBag.ErrorMessage";

				    var notice = new PNotify({
					    title: "@UIResources.ErrorMessage",
					    text: error,
					    width: "400px",
					    type: 'error',
					    addclass: 'stack-bottomright',
					    delay: 2000
				    });

				    notice.get().click(function () {
					    notice.remove();
				    });
			    }
            }

    </script>
    <script type="text/javascript">
        $(document).ready(function () {
          $('#loadingbar').hide();
          $('#tbDate').datepicker({
                format: "dd-M-yyyy",
                changeMonth: true,
                changeYear: true
          });
            $('#tbDateStart').datepicker({
                format: "dd-MM-yyyy",
                changeMonth: true,
                changeYear: true
          });
            $("[name=ProdCenterID]").on("change", function () {
                let opt = '<option label="Select Machine"></option><option value="All">All</option>';
                const val = $(this).val();

                $.ajax({
                    type: "post",
                    url: fastApp.BaseUrl + "/ShiftDaily/GetLinkUpList",
                    data: {
                        prodcenterID: $("[name=ProdCenterID]").val(),
                    },
                    datatype: "json",
                    traditional: true,
                    success: function (result) {
                        console.log(result);
                        if (result.Status == "True") {
                            for (let i = 0; i < result.Data.length; i++)
                            opt += `<option value="${result.Data[i]}">${result.Data[i]}</option>`;
                            $("[name=machine]").html(opt);
                        }
                        $('#loading_overlay').addClass('d-none');
                    },
                    complete: function () {
                        $('#loading_overlay').addClass('d-none');
                    },
                });
            })
            $('#ddlProdCenter2').change(function () {

			if ($('#ddlProdCenter2').val() != "0") {
				$("#tbLocationID2").val($('#ddlProdCenter2').val());
				$("#tbLocationType2").val('productioncenter');
			} else {
				$("#tbLocationType2").val('country');
			}

			$.ajax({
				type: "post",
				url: fastApp.BaseUrl + "/User/GetDepartmentByProdCenterID",
				data: { id: $('#ddlProdCenter2').val() },
				datatype: "json",
				traditional: true,
				success: function (data) {
					var ddlTemp = "<select id='ddlDepartment1'>";
					for (var i = 0; i < data.length; i++) {
						ddlTemp = ddlTemp + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
					}
					ddlTemp = ddlTemp + '</select>';
					$('#ddlDepartment2').html(ddlTemp);
					$('#ddlSubDepartment2').html("<select id='ddlSubDepartment'><option value='0'>- Please Select -</option></select>");
				}
			});
		});

		$('#ddlDepartment2').change(function () {

			if ($('#ddlDepartment2').val() != "0") {
				$("#tbLocationID2").val($('#ddlDepartment2').val());
				$("#tbLocationType2").val('department');
			} else {
				$("#tbLocationType2").val('productioncenter');
			}

			$.ajax({
				type: "post",
				url: fastApp.BaseUrl + "/User/GetSubDepartmentByDepartmentID",
				data: { id: $('#ddlDepartment2').val() },
				datatype: "json",
				traditional: true,
				success: function (data) {
					var ddlTemp = "<select id='ddlSubDepartment1'>";
					for (var i = 0; i < data.length; i++) {
						ddlTemp = ddlTemp + '<option value=' + data[i].Value + '>' + data[i].Text + '</option>';
					}
					ddlTemp = ddlTemp + '</select>';
					$('#ddlSubDepartment2').html(ddlTemp);
				}
			});
		});

		    $('#ddlSubDepartment2').change(function () {

			    if ($('#ddlSubDepartment2').val() != "0") {
				    $("#tbLocationID2").val($('#ddlSubDepartment2').val());
				    $("#tbLocationType2").val('subdepartment');
			    } else {
				    $("#tbLocationType2").val('department');
			    }
            });
        
            $('#btnSearch').on('click', function () {
                dateFilter = $("[id=tbDate]").val();
                prodCenter = $('[name=ProdCenterID]').val();
                machine = $("[name=machine]").val();
                
                if (dateFilter == "") {
                    showError("Date can't be empty");
                    return;
                }
                if (prodCenter == "") {
                    showError("Production Center can't be empty");
                    return;
                }
                if (machine == "") {
                    showError("Machine can't be empty");
                    return;
                }

                
              var table = $('#inputList').dataTable({
				"autoWidth": false,
				"processing": true,
				"serverSide": true,
				"filter": true,
				"orderMulti": false,
                "pageLength": 10,
                "destroy": true,
				"ajax": {
                    "url": fastApp.BaseUrl + "/InputDaily/GetAll",
                    "data": { dtFilter: dateFilter, prodCenter: prodCenter , machine: machine},
					"type": "POST",
					"datatype": "json"
				},

				"columnDefs":
					[{
						"targets": [0],
                        "searchable": false,
						"visible": false,
						"orderable": false
					},
					{
						"targets": [1],
						"searchable": true,
						"orderable": false
					},
					{
						"targets": [2],
						"searchable": true,
						"orderable": false
					},
					{
						"targets": [3],
						"searchable": true,
						"orderable": false
					},
					{
						"targets": [4],
						"searchable": true,
						"orderable": false
                        },
					{
						"targets": [5],
						"searchable": true,
						"orderable": false
                        },
                    {
						"targets": [6],
						"searchable": true,
						"orderable": false
                        },
                      {
						"targets": [7],
						"searchable": true,
						"orderable": false
                        } ,
					{
						"targets": [8],
						"searchable": true,
						"orderable": false
                        },
                    {
						"targets": [9],
						"searchable": true,
						"orderable": false
                        },
                      {
